{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": 0,
        "comma-dangle": 0,
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "jsx-a11y/alt-text": 0,
        "react/no-array-index-key": 0,
        "no-debugger": 1,
        "jsx-a11y/accessible-emoji": 0
    }
}
